<!-- 지시자  -->
<%@ page language="java" contentType="text/html; charset=UTF-8"
    pageEncoding="UTF-8"%>
    
<!-- 스크립틀릿 -->    
<% 
	String userName = (String)request.getAttribute("userName"); 
	
	int sum = 0;
	for(int i=0; i<10; i++) {
		sum += i;
	}

%>
<!DOCTYPE html>
<html>
<head>
<meta charset="UTF-8">
<title>Insert title here</title>
</head>
<body>
	<!-- 표현식  -->
	<p> <%= sum %></p>
	<p> 주문자 이름 : <%=userName%></p>

</body>
</html>